        xmlns:local="clr-namespace:Empleado_23CV"
        mc:Ignorable="d"
        Title="MainWindow" Height="650" Width="400">
    <Grid>
        <Label Content="Empleado" HorizontalAlignment="Left" Height="25" Margin="146,35,0,0" VerticalAlignment="Top" Width="129"/>
        <Label Content="Nombre" HorizontalAlignment="Left" Margin="49,107,0,0" VerticalAlignment="Top" Width="58"/>
        <TextBox x:Name="txtNombre" HorizontalAlignment="Left" Height="20" Margin="49,138,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="145"/>
        <Label Content="Apellido" HorizontalAlignment="Left" Height="31" Margin="49,177,0,0" VerticalAlignment="Top" Width="58"/>
        <Label Content="Correo" HorizontalAlignment="Left" Height="31" Margin="49,251,0,0" VerticalAlignment="Top" Width="58"/>
        <TextBox x:Name="txtCorreo" HorizontalAlignment="Left" Height="20" Margin="49,282,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="145"/>
        <Label Content="Fecha de ingreso" HorizontalAlignment="Left" Height="31" Margin="49,323,0,0" VerticalAlignment="Top" Width="133"/>
        <TextBox x:Name="txtApellido" HorizontalAlignment="Left" Height="20" Margin="49,208,0,0" Text="" VerticalAlignment="Top" Width="145"/>
