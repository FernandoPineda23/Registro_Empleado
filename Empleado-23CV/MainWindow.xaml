        <Button Content="Eliminar" HorizontalAlignment="Center" Margin="0,553,0,0" VerticalAlignment="Top" Height="23" Width="64" Click="Button_Click_2"/>
        <TextBox x:Name="txtID" HorizontalAlignment="Left" Height="20" Margin="49,430,0,0" Text="" VerticalAlignment="Top" Width="145"/>
        <Button Content="Editar" HorizontalAlignment="Center" Margin="0,525,0,0" VerticalAlignment="Top" Height="23" Width="64" Click="Button_Click_1"/>
        <Button Content="Agregar" HorizontalAlignment="Center" Margin="0,497,0,0" VerticalAlignment="Top" Height="23" Width="64" Click="Button_Click"/>
        <Button Content="Ver" HorizontalAlignment="Center" Margin="0,469,0,0" VerticalAlignment="Top" Height="23" Click="Button_Ver_Click" Width="64"/>
        <Button Content="Limpiar" HorizontalAlignment="Center" Margin="0,581,0,0" VerticalAlignment="Top" Width="64" Click="Button_Click_3"/>

    </Grid>
</Window>
